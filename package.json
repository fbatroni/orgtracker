{
  "name": "orgtracker",
  "version": "1.0.0",
  "description": "Organization Tracker",
  "readme": "README.md",
  "main": "./server.js",
  "scripts": {
    "build": "npm run clean && babel ./src --out-dir dist --copy-files",
    "lint": "eslint --ext .js --rule 'prettier/prettier: 2' src/ test/ *.js",
    "lint-staged": "lint-staged",
    "start": "npm run build && node ./dist/index.js",
    "local": "./scripts/run-local.sh",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000",
    "clean": "rimraf dist"
  },
  "url": "https://github.com/fbatroni/orgtracker",
  "repository": {
    "type": "git",
    "url": "git@github.com:fbatroni/orgtracker.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  },
  "author": "Fritz G. Batroni",
  "license": "UNLICENSED",
  "dependencies": {
    "@babel/core": "7.7.4",
    "@babel/polyfill": "^7.10.4",
    "@babel/register": "^7.10.5",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "config": "3.2.0",
    "connect-redis": "4.0.3",
    "cookie-parser": "1.4.4",
    "cookie-session": "1.3.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "express": "4.17.1",
    "express-session": "1.17.0",
    "express-winston": "^4.0.5",
    "helmet": "3.21.3",
    "ioredis": "^4.17.3",
    "log4js": "5.1.0",
    "pg": "^8.3.0",
    "querystring": "0.2.0",
    "sequelize": "^6.3.4",
    "uuid": "3.3.3",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/node": "7.7.4",
    "@babel/plugin-proposal-class-properties": "7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "7.8.3",
    "@babel/plugin-proposal-optional-chaining": "7.9.0",
    "@babel/plugin-transform-runtime": "7.7.4",
    "@babel/preset-env": "7.7.4",
    "@babel/runtime": "7.6.0",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "cross-env": "^7.0.2",
    "eslint": "6.5.1",
    "eslint-config-prettier": "6.4.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "3.0.9",
    "jest": "^26.2.2",
    "lint-staged": "9.4.2",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "webpack": "4.42.0",
    "webpack-cli": "3.3.11"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "<rootDir>/test/jest-set-up/index.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "engines": {
    "node": ">= 12.4.0"
  }
}
